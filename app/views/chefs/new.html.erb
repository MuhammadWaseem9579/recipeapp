<br>
<%= render 'layouts/errormsg' , obj: @chef %>
<%= form_for @chef do |f| %>
  <div class="mb-3">
    <%= f.label :name, class: "form-label" %>
    <%= f.text_field :name , class: "form-control" , id:"exampleInputEmail1", placeholder: "Enter your name" %>
  </div>
  <div class="mb-3">
    <%= f.label :email, class: "form-label" %>
    <%= f.text_field :email , class: "form-control" , id:"exampleInputEmail1" , placeholder: "Enter your email" %>
  </div>
  <div class="mb-3">
    <%= f.label :password, class: "form-label" %>
    <%= f.password_field :password_digest , class: "form-control" , id:"exampleInputEmail1" , placeholder: "Enter your password" %>
  </div>

  <%= f.submit "Sign Up", class: "btn btn-primary "%>
<% end %>